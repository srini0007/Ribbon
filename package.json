{
  "name": "ribbon-menu-new",
  "description": "Ribbon Menu for React (Vite, TypeScript)",
  "version": "0.1.17",
  "license": "MIT",
  "author": "srini",
  "keywords": [
    "react",
    "ribbon",
    "ribbon-menu",
    "js",
    "less",
    "css",
    "html"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
